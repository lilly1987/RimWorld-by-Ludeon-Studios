<?xml version="1.0" encoding="utf-8"?>
<Preset>
  <Info>
    <Version>1.0</Version>
    <Author>neitsa</Author>
    <Template>true</Template>
    <Description>A set of filters for an easy start!</Description>
  </Info>
  <Filters>
    <Terrain>
      <Biome>TemperateForest</Biome>
      <Stones>
        <Stone>
          <defName>Granite</defName>
          <State>On</State>
        </Stone>
        <Stone>
          <defName>Marble</defName>
          <State>On</State>
        </Stone>
        <Stone>
          <defName>Slate</defName>
          <State>Partial</State>
        </Stone>
        <Stone>
          <defName>Limestone</defName>
          <State>Off</State>
        </Stone>
        <Stone>
          <defName>Sandstone</defName>
          <State>Off</State>
        </Stone>
      </Stones>
      <CoastalTile>
        <State>On</State>
      </CoastalTile>
    </Terrain>
    <Temperature>
      <GrowingPeriod>
        <Use>true</Use>
        <Min>Tenth</Min>
        <Max>Twelfth</Max>
      </GrowingPeriod>
    </Temperature>
  </Filters>
  <Options>
    <DisablePreFilterCheck>true</DisablePreFilterCheck>
  </Options>
</Preset>